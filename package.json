    "dev": "cross-env ENV=DEV cross-env DB_SYNC=false nest start --watch",
    "dev:syncdb": "cross-env ENV=DEV cross-env DB_SYNC=true nest start --watch",
    "prod": "cross-env ENV=PROD cross-env DB_SYNC=false nest start --watch",
    "prod:syncdb": "cross-env ENV=PROD cross-env DB_SYNC=true nest start --watch",
    "start:syncdb": "SYNC_DB=true nest start --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:prod": "pm2 start ecosystem.config.json",
    "start:dev": "pm2 start devEcosystem.config.json",
    "stop:prod": "pm2 stop dist/src/main.js --name hk-backend1 -p 4000",
    "stop:dev": "pm2 stop devEcosystem.config.json",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vm": "ssh -i ../pk/Dev/hzk.pem ubuntu@ec2-3-108-249-37.ap-south-1.compute.amazonaws.com",
    "vm:db": "ssh -i ../pk/Dev/hzk.pem ubuntu@ec2-3-108-190-131.ap-south-1.compute.amazonaws.com",
    "vm:dev-db": "ssh -i ../pk/Dev/hzk.pem ubuntu@ec2-43-204-150-219.ap-south-1.compute.amazonaws.com",
    "vm:backup-db": "ssh -i ../pk/Dev/hzk.pem ubuntu@ec2-43-204-116-226.ap-south-1.compute.amazonaws.com",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "customSeed:run": "node dist/src/seeds/customSeeder.js",
    "dev:schema-drop": "typeorm schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "deploy:dev": "",
    "deploy:web": "scp -i ../pk/Dev/hzk.pem ../WEB/ReactNative/dist/* ubuntu@ec2-3-108-249-37.ap-south-1.compute.amazonaws.com:~/hzk/HKDevWebBuild/"
